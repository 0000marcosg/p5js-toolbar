{
  "name": "p5js-toolbar",
  "main": "./lib/p5js-toolbar",
  "version": "0.1.4",
  "description": "A small toolbar for running p5js sketches & other helpful p5js information",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "p5js-toolbar:toggle",
      "p5js-toolbar:start",
      "p5js-toolbar:stop",
      "p5js-toolbar:reference",
      "p5js-toolbar:report-issue",
      "p5js-toolbar:forum",
      "p5js-toolbar:new-project"
    ]
  },
  "repository": "https://github.com/bmoren/p5js-toolbar",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "opn": "^5.2.0",
    "portfinder": "^1.0.13",
    "readdirp": "^2.1.0"
  }
}
